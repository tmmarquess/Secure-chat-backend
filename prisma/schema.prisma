generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid())
  name     String?
  email    String  @unique
  password String
}

model pubkeys {
  user_id String @id @db.VarChar(255)
  pubkey  String @db.VarChar(255)
}
